{
	"private": true,
	"name": "github-user-visualization",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"prepare": "pnpm run smui-theme-light && pnpm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme -i ./node_modules",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark -i ./node_modules",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write . --ignore-path .gitignore",
		"fetch": "tsx scripts/fetch.ts",
		"move": "tsx scripts/move.ts"
	},
	"devDependencies": {
		"@jacoblincool/puddle": "^0.1.1",
		"@playwright/test": "^1.28.1",
		"@smui/banner": "^6.1.4",
		"@smui/button": "^6.1.4",
		"@smui/common": "^6.1.4",
		"@smui/linear-progress": "^6.1.4",
		"@smui/top-app-bar": "^6.1.4",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@types/d3": "^7.4.0",
		"@types/node": "^18.11.15",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"dotenv": "^16.0.3",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"node-fetch": "^3.3.0",
		"octokit": "^2.0.10",
		"ora": "^6.1.2",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"smui-theme": "^6.2.0",
		"svelte": "^3.55.0",
		"svelte-check": "^2.10.2",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.1",
		"vitest": "^0.25.8"
	},
	"dependencies": {
		"d3": "^7.7.0"
	}
}
